local Rayfield = loadstring(game:HttpGet('https://sirius.menu/rayfield'))()

local ClickSound = Instance.new("Sound")
ClickSound.SoundId = "rbxassetid://535716488"
ClickSound.Volume = 1
ClickSound.Parent = game.Players.LocalPlayer:WaitForChild("PlayerGui")
function PlayClick()
    ClickSound:Play()
end

local Window = Rayfield:CreateWindow({
    Name = "MozCraft",
    Icon = 124529086555369, 
    LoadingTitle = "MozCraft: The Blocks Update",
    LoadingSubtitle = "SkyeMoz",
    ShowText = "Creative UI",
    Theme = "DarkBlue",
    ToggleUIKeybind = "Z",
    DisableRayfieldPrompts = false,
    DisableBuildWarnings = false,

    ConfigurationSaving = {
        Enabled = true,
        FolderName = nil, 
        FileName = "Hub"
    },

    Discord = {
        Enabled = false, 
        Invite = "wKVgf2EDGZ",
        RememberJoins = true
    },

    KeySystem = false,  
    KeySettings = {
        Title = "MozCraft ",
        Subtitle = "Key System",
        Note = "No method of obtaining the key is provided",
        FileName = "Key",
        SaveKey = true,
        GrabKeyFromSite = false,
        Key = {"Key_41414"}  
    }
})

local Tab = Window:CreateTab("Info", 9405926389)
local Section = Tab:CreateSection("Social")
local Button = Tab:CreateButton({
   Name = "Discord (Recommended)",
   Callback = function()
     PlayClick()  
     setclipboard("https://discord.gg/wKVgf2EDGZ") 
     Rayfield:Notify({Title = "Discord", Content = "Copied!", Duration = 2})
   end,
})
